{
  "version": 2,
  "buildCommand": "vite build --outDir dist client",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.ts"
    },
    {
      "src": "/api/cv/download", 
      "dest": "/api/cv/download.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}